      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_ANALYZER_POSTGRESQL}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH risk_analysis AS (\n  SELECT \n    source_address as \"üîç Source IP\",\n    destination_address as \"üéØ Target IP\",\n    protocol as \"üîå Protocol\",\n    destination_port as \"üö™ Port\",\n    COUNT(*) as connection_count,\n    SUM(packet_count) as total_packets,\n    SUM(byte_count) as total_bytes,\n    COUNT(DISTINCT destination_port) as unique_ports,\n    -- Risk scoring algorithm\n    CASE \n      WHEN destination_port IN (22, 23, 3389, 21, 135, 445, 139) THEN 40  -- High-risk ports\n      WHEN destination_port > 50000 THEN 20  -- Very high ports\n      WHEN protocol = 'icmp' AND COUNT(*) > 50 THEN 60  -- ICMP flooding\n      ELSE 10\n    END +\n    CASE \n      WHEN SUM(packet_count) > 1000 THEN 30\n      WHEN SUM(packet_count) > 100 THEN 15\n      ELSE 5\n    END +\n    CASE \n      WHEN SUM(byte_count) > 10000000 THEN 25  -- > 10MB\n      WHEN SUM(byte_count) > 1000000 THEN 10   -- > 1MB\n      ELSE 2\n    END as risk_score\n  FROM connections \n  WHERE $__timeFilter(time) AND hostname IN ($hostname) \n    AND source_address IS NOT NULL\n    AND destination_address IS NOT NULL\n  GROUP BY source_address, destination_address, protocol, destination_port\n  HAVING COUNT(*) > 1\n)\nSELECT \n  \"üîç Source IP\",\n  \"üéØ Target IP\",\n  \"üîå Protocol\",\n  \"üö™ Port\",\n  connection_count as \"üîó Connections\",\n  total_packets as \"üì¶ Packets\",\n  total_bytes as \"üìä Bytes\",\n  risk_score as \"üö® Risk Score\"\nFROM risk_analysis\nWHERE risk_score > 30\nORDER BY risk_score DESC\nLIMIT 25",
          "refId": "A"
        }
      ],
      "title": "üö® High-Risk Network Activity",
      "type": "table"
